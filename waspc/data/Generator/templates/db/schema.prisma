{{={= =}=}}

datasource db {
  provider = "{= datasourceProvider =}"
  url      = {=& datasourceUrl =}
}

generator client {
  provider = "prisma-client-js"
  {=! TODO(matija): this shouldn't be hardcoded, generator should provide this path. =}
  output = "../server/node_modules/.prisma/client"
}

{=# modelSchemas =}
{=& . =}
{=/ modelSchemas =}

{=# isAuthEnabled =}
model Session {
  id String @id
  sid String @unique
  data String
  expiresAt DateTime
}
{=/ isAuthEnabled =}
